{
  "name": "dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project dashboard -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:ce": "ng serve --project external-dashboard-tile -o",
    "build:ce": "npm run bundle:ce && npm run copy:ce",
    "copy:ce": "cpr dist/external-dashboard-tile/main.js src/assets/external-dashboard-tile.bundle.js -o",
    "bundle:ce": "ng build --project external-dashboard-tile --prod --output-hashing none",
    "build:modern": "ng build --plugin ngx-build-modern --prod",
    "build:modern:external-dashboard-tile": "ng build --plugin ngx-build-modern --project external-dashboard-tile --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/router": "^7.0.1",
    "@swimlane/ngx-charts": "^8.1.0",
    "@webcomponents/custom-elements": "^1.1.2",
    "application": "^0.1.4",
    "core-js": "^2.5.4",
    "install": "^0.12.1",
    "ngx-build-modern": "^1.0.2",
    "npm": "^6.1.0",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/elements": "^7.0.1",
    "@angular/language-service": "^7.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cpr": "^3.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-build-plus": "^7.2.3",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.3"
